{
	"name" : "ekliptor/php-grpc",
	"description" : "A BitcoinCash (BCH) wallet using BCHD and bchwallet.",
	"keywords" : [
		"bitcoin",
		"cryptocurrency",
		"billing",
		"ecommerce",
		"payment"
	],
	"type" : "library",
	"require" : {
		"grpc/grpc" : "^1.27",
		"google/protobuf": "^3.12"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.2"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "Ekliptor",
			"email" : "Ekliptor1@yandex.com"
		}
	],
	"minimum-stability" : "stable",
	"prefer-stable" : true,
	"autoload": {
		"psr-4": {
			"Ekliptor\\grpc\\": ["src/"],
			"GPBMetadata\\": ["proto-php/GPBMetadata/"],
			"Walletrpc\\": "proto-php/Walletrpc/"
		}
	}
}
